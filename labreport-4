#include <iostream>
using namespace std;

int main() {
    int playlist[100], size, songID, pos = -1;
    cout << "Enter number of songs in playlist: ";
    cin >> size;
    cout << "Enter ID of " << size << " songs"<<endl;
    for (int i = 0; i < size; i++) {
        cin >> playlist[i];
    }
    cout << "Enter song ID to search: ";
    cin >> songID;
    for (int i = 0; i < size; i++) {
        if (playlist[i] == songID) {
            pos = i;
            break;}
    }
    if (pos != -1) {
        for (int i = size; i > pos + 1; i--) {
            playlist[i] = playlist[i - 1];
        }
        playlist[pos + 1] = -1;
        size++;
        cout << "Updated playlist: ";
        for (int i = 0; i < size; i++) {
            cout << playlist[i] << " ";
        }
        cout << endl;
    } else {
        cout << "NOT FOUND" << endl;
    }
    return 0;}
